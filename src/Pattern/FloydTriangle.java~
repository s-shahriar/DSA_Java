package Pattern;

public class FloydTriangle {
    public static void main(String[] args) {
        int totalRows = 5;
        int number = 1;

        for (int i = 1; i <= totalRows; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print(number+ " ");
                number++;
            }
            System.out.println();
        }
    }
}
