package Pattern;

public class InvertedHalfPyramidNumber {
    public static void main(String[] args) {
        int totalRows = 5;

        for (int i = 1; i <= totalRows; i++) {
            for (int j = 1; j <= totalRows-i+1; j++) {
                System.out.print(j);
            }
            System.out.println();
        }
    }
}
